all:
	make -C common/
	make -C usa/
	make -C usc/
	make -C usd/
	make -C use/
	make -C usm/

install:
	make -C usa/ install
	make -C usc/ install
	make -C usd/ install
	make -C use/ install
	make -C usm/ install

clean:
	make -C common/ clean
	make -C usa/ clean
	make -C usc/ clean
	make -C usd/ clean
	make -C use/ clean
	make -C usm/ clean
